<p id="notice"><%= notice %></p>

<%= page_entries_info @chemicals %>
<h1>Chemicals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @chemicals.each do |chemical| %>
      <tr>
        <td><%= link_to chemical.name, chemical %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Chemical', new_chemical_path %>

<%= paginate @chemicals %>
